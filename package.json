{
  "name": "@cypsela/fs-entry-source",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "files": [
    "./dist/src"
  ],
  "keywords": ["ipfs", "unixfs", "browser", "FileSystemEntry"],
