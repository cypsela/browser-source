  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typedoc": "^0.28.5",
    "typescript": "^5.8.3",
    "vitest": "^3.2.2"
  }
